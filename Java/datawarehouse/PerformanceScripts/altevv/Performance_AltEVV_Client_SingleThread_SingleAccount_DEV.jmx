        <JSR223Sampler guiclass="TestBeanGUI" testclass="JSR223Sampler" testname="Generate Payload  for NumberOfRuns ${numberOfRuns} - NumberOfAccounts ${numberOfAccounts} - NumberOfRecords ${numberOfRecords}" enabled="true">
          <stringProp name="cacheKey">true</stringProp>
          <stringProp name="filename"></stringProp>
          <stringProp name="parameters"></stringProp>
          <stringProp name="script">import com.perf.AltEvvDataGenerator;

System.setProperty(&quot;DefaultENVID&quot;,&quot;PA&quot;)
System.setProperty(&quot;environment&quot;,&quot;DEV&quot;)
int numberOfRuns = Integer.parseInt(vars.get(&quot;numberOfRuns&quot;));
int numberOfAccounts = Integer.parseInt(vars.get(&quot;numberOfAccounts&quot;));
int numberOfRecords = Integer.parseInt(vars.get(&quot;numberOfRecords&quot;));

for (int i=0;i&lt; numberOfRuns; i++){
	log.info(&quot;Generate Client Data for Run No. &quot; + i);
	String payload = AltEvvDataGenerator.generate5kAltEvvClientRecords(numberOfAccounts, numberOfRecords,0);	
	String varName = &quot;payload_&quot;+i;
	log.info(&quot;---- Store client payload into JMeter var: &quot; + varName); 	
	vars.put(varName, payload);
	log.info(&quot;--------Done&quot;);
}
</stringProp>
          <stringProp name="scriptLanguage">groovy</stringProp>
